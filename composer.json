{
    "name": "gerardbalaoro/laravel-installer",
    "description": "Alternative Laravel Application Installer",
    "keywords": ["laravel", "installer"],
    "license": "MIT",
    "authors": [{
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Gerard Balaoro",
            "email": "gmbalaoro@outlook.com"
        }
    ],
    "require": {
        "ext-zip": "*",
        "composer/semver": "^1.5",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/console": "~3.0|~4.0|~5.0",
        "symfony/filesystem": "~3.0|~4.0|~5.0",
        "symfony/process": "~3.0|~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/laravel"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Installer\\": "src/",
            "Laravel\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "replace": {
        "laravel/installer": "*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}